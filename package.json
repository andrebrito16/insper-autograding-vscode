{
  "name": "insper-autograding-new",
  "displayName": "insper-autograding-new",
  "description": "Insper Autograding Toolbox",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "insper-autograding-new.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "insper-autograding.showAlert",
        "title": "Insper Autograding: Show Alert",
        "icon": {
          "light": "resources/check.svg",
          "dark": "resources/check-white.svg"
        }
      },
      {
        "command": "insper-autograding.clearToken",
        "title": "Insper Autograding: Clear Token"
      },
      {
        "command": "insper-autograding.setToken",
        "title": "Insper Autograding: Set Token"
      }
    ],
    "menus": {
      "notebook/cell/title": [
        {
          "command": "insper-autograding.showAlert",
          "when": "notebookType == jupyter-notebook && notebookCellType == code",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.7.2"
  }
}
